#include <stdio.h>

int main(int argc, char *argv[])
{
    int N1;
    int N2;
    do
    {
        printf("Inserisci N1: ");
        scanf("%d", &N1);
        printf("Inserisci N2: ");
        scanf("%d", &N2);
    }while(N2<N1 || N1<=0 || N2<=0);

    for(int i=N2; i>=N1; i--)
    {
        printf("%d\n", i);
    }
 
return 0;    
}
