int main()
{
	int a;
	int b;
	float rapporto;
	
	do
	{
		printf("Inserisci primo numero: ");
		scanf("%d", &a);
		printf("Inseisci secondo numero: ");
		scanf("%d", &b);
		
		if(a!=0 && b!=0)
		{
			if(a<=b)
			{
				rapporto= (float)a/b;
			}
			else if(b<=a)
			{
				rapporto=(float)b/a;
				
			}
			printf("Il rapporto e': %.2f\n", rapporto);
			
		}
		
	}while(a!=0 && b!=0);
	
	return 0;
}
