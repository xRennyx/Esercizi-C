#include <stdio.h>

int main()
{
    int a;
	int b; 
	int n;
	int i = 0; 
	int somma = 0; 
	float media;
	do
	{
		printf("inserisci a: ");
		scanf("%d", &a);
		printf("inserisci b: ");
		scanf("%d", &b);
	} while (b<a);

	do
	{
		printf("inserisci un numero: ");
		scanf("%d", &n);

		if(n>=a && n<=b)
		{
			somma=somma+n;
			i++;
		}

	} while (n>=a && n<=b);
	media = (float)somma/i;

	printf("la media e': %.2f\n", media);

	return 0;
}
