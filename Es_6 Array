#include <stdio.h>
#include <stdlib.h>
#include <string.h>


int main(int argc, char *argv[])
{
	int i, len = 0, k = 0, j;
	int a=0;

	for (i = 1; i < argc; i++)
	{
		len = len + strlen(argv[i]);
	}

	len = len + argc - 2;
	printf("la stringa Ã¨ lunga %d\n", len);

	char str[len];

	for (i = 1; i < argc; i++)
	{

		for (j = 0,k=len; j < strlen(argv[i]); j++, k--)
		{
			if(str[k] == argv[i][j])
			{
			    a++;
			}
		}
		
	}

    if(a==len)
    {
        printf("1");
    }else
    {
        printf("0");
    }
	

	return 0;
}
