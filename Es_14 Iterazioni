#include <stdio.h>

int main(int argc, char *argv[])
{
    int N1;
    int N2;
    int somma=0;
    do
    {
        printf("Inserisci N1: ");
        scanf("%d", &N1);
        printf("Inserisci N2: ");
        scanf("%d", &N2);
    }while(N1<0 || N2<0);


    for(int i=0; i<N2; i++)
    {
        
        somma=somma+N1;  
    }
    printf("Il prodotto dei 2 numeri: %d", somma);
 
return 0;    
}
