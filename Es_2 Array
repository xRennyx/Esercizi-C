#include <stdio.h>

int main(int argc, char *argv[])
{
    int a[5];
    
    for(int i=0; i<5; i++)
    {
        printf("Inserisci %d° numero: ",i+1);
        scanf("%d", &a[i]);
    }
    printf("Il vettore è composto  da: ");
    
    for(int i=4; i>=0; i--)
    {
        printf("%d ",a[i]);
    }

    return 0;
}
